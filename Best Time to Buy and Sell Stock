#include <bits/stdc++.h> 
int maximumProfit(vector<int> &prices){
     int profit =0;
     int min = INT_MAX;
     for(int i=0;i<prices.size();i++)
     { 
        if(prices[i]<min){
          min = prices[i];
         }
        if(profit < prices[i] - min)
        {
            profit = prices[i] - min;
        }
    }
    return profit;
    
    // Write your code here.
}
