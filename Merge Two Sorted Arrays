#include <bits/stdc++.h>

vector<int> ninjaAndSortedArrays(vector<int>& nums1, vector<int>& nums2, int m, int n) {
	int l=m+n;
        for(int i=0,j=l-1;i<m,j>=m;i++,j--)
        {
                nums1[j]=nums2[i];
        }
        sort(nums1.begin(),nums1.end());
		return nums1;
	// Write your code here.
}
